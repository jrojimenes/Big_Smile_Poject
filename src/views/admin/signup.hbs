 <!-- row -->
  <section id="main-content">
      <section class="wrapper">
        <div class="row mt">
          <div class="col-md-12">
            <div class="content-panel">
              <table class="table table-striped table-advance table-hover">
                <h4><i class="fa fa-angle-right"></i> Gestión de Usuarios</h4>
                <hr>
                <thead>
                  <tr>
                    <th><i class="fa fa-bullhorn"></i> Nombre</th>
                    <th><i class="fa fa-bullhorn"></i> Email</th>
                    <th><i class="fa fa-bullhorn"></i> Teléfono</th>
                    <th><i class="fa fa-bullhorn"></i> Perfil</th>
                    <th><i class="fa fa-bullhorn"></i> Email</th>
                    <th><i class="fa fa-bullhorn"></i> Alias</th>
                    <th><i class=" fa fa-edit"></i> Status</th>
                    <th></th>
                  </tr>
                </thead>
                <tbody>
                  {{#each infoManage.listUsers}}
                  <tr>
                    <td>
                      <a href="basic_table.html#">{{fcUserName}} {{fcUserSecondName}} {{fcUserSecondName}}</a>
                    </td>
                    <td class="hidden-phone">{{fcMail}}</td>
                    <td class="hidden-phone">{{fiPhoneNumber}} </td>
                    <td class="hidden-phone">{{fiIdProfile}} </td>
                    <td class="hidden-phone">{{fcMail}} </td>
                    <td class="hidden-phone">{{fcUserNickName}} </td>
                    <td><span class="label label-info label-mini">Due</span></td>
                    <td>
                      <button class="btn btn-success btn-xs"><i class="fa fa-check"></i></button>
                      <button class="btn btn-primary btn-xs"><i class="fa fa-pencil"></i></button>
                      <button class="btn btn-danger btn-xs"><i class="fa fa-trash-o "></i></button>
                    </td>
                  </tr>
                  {{/each}}
                </tbody>
              </table>
            </div>
            <!-- /content-panel -->
          </div>
          <!-- /col-md-12 -->
        </div>
        </section>
        </section>
