    <section id="main-content">
      <section class="wrapper">
       <div class="row mt">
          <div class="col-lg-6">
            <h4><i class="fa fa-angle-right"></i> Alta de Usuarios</h4>
            <div class="form-panel">
              <div class="form">
                <form class="cmxform form-horizontal style-form" action="/signup" method="POST">
                  <div class="form-group">
                    <label for="cname" class="control-label col-lg-2">Nombre</label>
                    <div class="col-lg-10">
                      <input class=" form-control" id="cname" name="usrname" minlength="2" type="text" required />
                    </div>
                  </div>
                  <div class="form-group ">
                    <label for="cemail" class="control-label col-lg-2">Apellido paterno</label>
                    <div class="col-lg-10">
                      <input class="form-control " id="usrsecondname" type="text" name="usrsecondname" required />
                    </div>
                  </div>
                  <div class="form-group ">
                    <label for="curl" class="control-label col-lg-2">Apellido materno</label>
                    <div class="col-lg-10">
                      <input class="form-control " id="usrlastname" type="text" name="usrlastname" required />
                    </div>
                  </div>
                  <div class="form-group ">
                    <label for="curl" class="control-label col-lg-2">E-mail</label>
                    <div class="col-lg-10">
                      <input class="form-control " id="email" type="email" name="mail" required />
                    </div>
                  </div>
                  <div class="form-group ">
                    <label for="curl" class="control-label col-lg-2">Teléfono</label>
                    <div class="col-lg-10">
                      <input class="form-control " id="phone" type="number" name="phone" required />
                    </div>
                  </div>
                  <div class="form-group ">
                    <label for="curl" class="control-label col-lg-2">Selecciona el perfil</label>
                    <div class="col-lg-10">
                      <select class="form-control" id="profile" type="number" name="profileId">
                        <option>----Selecciona un perfil----</option>
                          {{#each infoManage.listProfiles}}
                          <option value="{{fiIdProfile}}">{{fcProfileName}}</option>
                          {{/each}}
                      </select>
                    </div>
                  </div>
                  <div class="form-group ">
                    <label for="curl" class="control-label col-lg-2">Alias de usuario</label>
                    <div class="col-lg-10">
                      <input class="form-control " id="nickName" type="text" name="nickName" required />
                    </div>
                  </div>
                  <div class="form-group ">
                    <label for="curl" class="control-label col-lg-2">Contraseña</label>
                    <div class="col-lg-10">
                      <input class="form-control " id="psw" type="password" name="password" required/>
                    </div>
                  </div>
                  <div class="form-group">
                    <div class="col-lg-offset-2 col-lg-10">
                      <button class="btn btn-theme" type="submit">Save</button>
                      {{!-- <button class="btn btn-theme04" type="button">Cancel</button> --}}
                    </div>
                  </div>
                </form>
              </div>
            </div>
            <!-- /form-panel -->
          </div>
          <!-- /col-lg-12 -->
        </div>
        </section>
        </section>
